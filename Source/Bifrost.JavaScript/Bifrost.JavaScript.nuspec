<?xml version="1.0"?>
<package >
  <metadata>
    <id>Bifrost.JavaScript</id>
    <version>$version$</version>
    <title>Bifrost JavaScript Support</title>
    <authors>Dolittle</authors>
    <owners>Dolittle</owners>
    <licenseUrl>http://github.com/dolittle/Bifrost/blob/master/MIT-LICENSE.txt</licenseUrl>
    <projectUrl>http://bifrost.dolittle.com</projectUrl>
    <iconUrl>http://bifrost.dolittle.com/img/bifrost-icon.png</iconUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>Support frontend development using JavaScript for Bifrost</description>
    <releaseNotes>http://bifr.st/ReleaseNotes</releaseNotes>
    <copyright>Copyright 2008 - 2017</copyright>
    <tags>MVVM LOB CQRS</tags>
    <dependencies>
      <dependency id="jQuery" version="2.2.3" />
      <dependency id="knockoutjs" version="3.4.0" />
	  <dependency id="RequireJS" version="2.1.11" />
    </dependencies>
  </metadata>
  <files>
    <file src="**\*.js" target="Content\Scripts\bifrost\" exclude="Bifrost.js;Bifrost.Debug.js;gulpfile.js;Libraries\**\*.*;node_modules\**\*.*"/>
    <file src="debug.html" target="Content\Scripts\bifrost\" />
  </files>
</package>